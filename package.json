{
  "name": "foundryvtt-rest-api-relay",
  "version": "2.0.13",
  "description": "Message relay server for the foundryvtt-rest-api Foundry Module",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "cross-env PORT=3010 dotenv -e .env tsx watch src/index.ts",
    "db:migrate": "tsx src/models/init.ts",
    "local:sqlite": "cross-env DB_TYPE=sqlite PORT=3010 tsx watch src/index.ts",
    "local": "cross-env DB_TYPE=memory PORT=3010 tsx watch src/index.ts",
    "postinstall": "npx puppeteer install",
    "reset-requests": "dotenv -e .env tsx scripts/reset-requests.ts",
    "usage-stats": "dotenv -e .env tsx scripts/usage-stats.ts",
    "docs:dev": "cd docs && pnpm start",
    "docs:build": "cd docs && pnpm build",
    "docs:install": "cd docs && pnpm install"
  },
  "author": "ThreeHats",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@clerk/backend": "^1.34.0",
    "@docusaurus/theme-search-algolia": "^3.8.1",
    "@stripe/stripe-js": "^5.10.0",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "handlebars": "^4.7.8",
    "ioredis": "^5.7.0",
    "jsdom": "^26.1.0",
    "multer": "1.4.5-lts.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "pino": "^9.7.0",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.6.2",
    "prom-client": "^15.1.3",
    "puppeteer": "^24.15.0",
    "redis": "^4.7.1",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "stripe": "^17.7.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@docusaurus/tsconfig": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "5.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.17.0",
    "@types/node-fetch": "^2.6.13",
    "@types/pg": "^8.15.5",
    "@types/ws": "^8.18.1",
    "cross-env": "^7.0.3",
    "docusaurus-plugin-typedoc": "^1.4.2",
    "dotenv": "^16.6.1",
    "dotenv-cli": "^8.0.0",
    "fvtt-types": "npm:@league-of-foundry-developers/foundry-vtt-types@latest",
    "sequelize-cli": "^6.6.3",
    "tsx": "^4.20.3",
    "typedoc": "^0.28.9",
    "typedoc-plugin-markdown": "^4.8.0",
    "typescript": "~5.7.3"
  }
}
